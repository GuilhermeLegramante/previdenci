{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "asantibanez/livewire-charts": "^2.4",
        "aws/aws-sdk-php": "^3.269",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "barryvdh/laravel-ide-helper": "*",
        "barryvdh/laravel-snappy": "^0.4.8",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "geekcom/validator-docs": "^3.5",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.6",
        "jeroennoten/laravel-adminlte": "^3.1",
        "kosinix/grafika": "^2.0",
        "laracasts/flash": "^3.1",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "livewire/livewire": "^2.3",
        "lucascudo/laravel-pt-br-localization": "^1.0",
        "mpdf/mpdf": "^8.0",
        "spatie/pdf-to-text": "^1.3",
        "symfony/var-dumper": "^5.4",
        "thiagoalessio/tesseract_ocr": "^2.12",
        "vajiral/php-image-compare": "^1.0",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
